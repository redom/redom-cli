{
  "name": "redom-example",
  "version": "0.0.1",
  "description": "RE:DOM example project",
  "main": "server.js",
  "scripts": {
    "dev": "node watch",
    "build-css": "stylus -w -u nib css/index.styl -c -o public/css/main.css",
    "build-js": "rollup -m -w -c -f iife js/index.js -o public/js/main.js",
    "uglify": "uglifyjs public/js/main.js -cm -o public/js/main.min.js",
    "start": "node server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@redom/store": "~1.0.0",
    "nib": "~1.1.2",
    "redom": "~3.8.8",
    "rollup": "~0.52.1",
    "rollup-plugin-buble": "~0.18.0",
    "rollup-plugin-butternut": "~0.1.0",
    "rollup-plugin-node-resolve": "~3.0.0",
    "stylus": "~0.54.5"
  },
  "dependencies": {
    "compression": "~1.7.1",
    "express": "~4.16.2"
  }
}
